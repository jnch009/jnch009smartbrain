{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "plugins": ["react"],
  "rules": {
      "jsx-quotes": ["error","prefer-single"],
      "indent": [2,2],
      "quotes": [2,"single"],
      "no-unused-vars": [1],
      "no-undef":"warn"
  }
}